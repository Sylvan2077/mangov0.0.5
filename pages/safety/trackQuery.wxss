.header {
  width:100%;
  height: 180rpx;
  background-color: #fff;
  text-align: center;
}
.header .row {
  width: 750rpx;
  margin-top:10rpx;
  display: flex;
  flex-direction: row;
}
.header .cell-1 {
  width: 255rpx;
}
.header .cell-2 {
  width: 255rpx;
}
.header .cell-3 {
  width: 240rpx;
}
.header .cell-1 input,.header .cell-2 input{
  border: 1px #ddd solid;
  width: 92%;
  border-radius: 10rpx;
  padding: 8rpx 0;
  margin-left: 2%;
}
.header .cell-3 button{
  width: 90%;
  padding: 2rpx 0;
}
.date-unit {
  width:92%;
  height:68rpx;
  line-height:68rpx;
  border:1px #ddd solid;
  margin-left:2%;
  font-size: 14px;
  color: #ccc;
  border-radius: 10rpx;
}
.time-unit {
  width:92%;
  height:68rpx;
  line-height:68rpx;
  border:1px #ddd solid;
  font-size: 14px;
  color: #ccc;
  border-radius: 10rpx;
}